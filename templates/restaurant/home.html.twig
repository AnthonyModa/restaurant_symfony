{% extends 'base.html.twig' %}

    {% block title %}
    Manger à Visé | Accueil
    {% endblock %}

    {% block head_css %}
    {% endblock %}


    {% block body %}
        <section id="content"><div class="ic"></div>
            <div class="container">
                <div class="row">
                    <div class="page1_block">
                        <div class="grid_6">
                            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d80794.5821064185!2d5.616350595109644!3d50.74162902405805!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47c0ee6427760e23%3A0x40099ab2f4d7200!2sVise!5e0!3m2!1sen!2sbe!4v1539093570918" width="600" height="450" style="border:0" allowfullscreen></iframe>
