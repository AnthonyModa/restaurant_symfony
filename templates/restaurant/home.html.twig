                                <div class="images">
                                    <img src="images/resto1.jpg" alt="" width="450" height="340">
                                    <img src="images/resto3.jpeg" alt="" width="450" height="340">
                                    <img src="images/resto3.jpeg" alt="" width="450" height="340">
                                </div>
                            </div>
                        </div>

                        <div class="grid_4">
                            <div data-si-mousemove-trigger="100" class="shuffle-me shuff__1">
                                <a href="{{ path('restaurant', {'id' : 29}) }}" class="info"><div class="shuffle_title">Les 4 Saisons D'Asie<span>En savoir plus</span></div></a>
                                <div class="images">
                                    <img src="images/page1_img5.jpg" alt="">
                                    <img src="images/shuffle_3.jpg" alt="">
                                    <img src="images/shuffle_4.jpg" alt="">
